<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	 http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="stu" class="com.Student._StudentsBean.StudentDetails"> 
		  <constructor-arg value="Mani"/>
		  <constructor-arg value="24"/>
		  <constructor-arg value="CSE"/>
		  
	</bean>
	<bean id="clg" class="com.Student._StudentsBean.CollegeDetails"> 
		  <constructor-arg value="SCCE"/>
		  <constructor-arg ref="stu"/>
	</bean>
</beans>